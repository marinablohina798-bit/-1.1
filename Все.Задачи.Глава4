###4.143
n,m = map(int, input().split())
days_in_month = [31,28,31,30,31,30,31,31,30,31,30,31]
if n > 1:
    print(n-1, m)
else:
    print(days_in_month[m-2], m-1)
if n < days_in_month[m-1]:
    print(n+1, m)
else:
    print(1, m+1)

###4.144
g,m,n = map(int, input().split())
is_leap = (g % 4 == 0 and g % 100 != 0) or (g % 400 == 0)
days_in_month = [31,29 if is_leap else 28,31,30,31,30,31,31,30,31,30,31]
if n > 1:
    print(g, m, n-1)
elif m > 1:
    print(g, m-1, days_in_month[m-2])
else:
    print(g-1, 12, 31)
if n < days_in_month[m-1]:
    print(g, m, n+1)
elif m < 12:
    print(g, m+1, 1)
else:
    print(g+1, 1, 1)

###4.145
n = int(input())
animals = ["Крыса","Корова","Тигр","Заяц","Дракон","Змея","Лошадь","Овца","Обезьяна","Петух","Собака","Свинья"]
colors = ["Зеленый","Красный","Желтый","Белый","Черный"]
year_in_cycle = (n - 4) % 60
color_index = (year_in_cycle // 2) % 5
animal_index = year_in_cycle % 12
print(f"{animals[animal_index]}, {colors[color_index]}")
