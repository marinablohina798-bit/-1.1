###4.80
a,b,c = map(float, input().split())
if a+b>c and a+c>b and b+c>a:
    sides = sorted([a,b,c])
    print("прямоугольный" if abs(sides[0]**2 + sides[1]**2 - sides[2]**2) < 1e-9 else "не прямоугольный")
else:
    print("не существует")

###4.81
a,b,c = map(float, input().split())
if a+b>c and a+c>b and b+c>a:
    sides = sorted([a,b,c])
    if abs(sides[0]**2 + sides[1]**2 - sides[2]**2) < 1e-9:
        print("прямоугольный", end=" ")
    elif sides[0]**2 + sides[1]**2 > sides[2]**2:
        print("остроугольный", end=" ")
    else:
        print("тупоугольный", end=" ")
    if a==b==c:
        print("равносторонний")
    elif a==b or b==c or a==c:
        print("равнобедренный")
    else:
        print("разносторонний")
else:
    print("не существует")

###4.82
n = int(input())
if 10 <= n % 100 <= 20:
    suffix = "лет"
elif n % 10 == 1:
    suffix = "год"
elif n % 10 in (2,3,4):
    suffix = "года"
else:
    suffix = "лет"
print(f"мне {n} {suffix}")

