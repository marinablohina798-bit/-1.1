###4.7
import math
x = 1
if math.sin(x) < 0:
    k = x ** 2
else:
    k = abs(x)
if k < x:
    f = k * x
else:
    f = k + x
print("f =", f)

###4.8
import math
x = 1
if math.sin(x) >= 0:
    k = x ** 2
else:
    k = abs(x)
if x < k:
    f = abs(x)
else:
    f = k * x
print("f =", f)

