###4.19
import math
s_circle = float(input())
s_triangle = float(input())
side_triangle = math.sqrt(4 * s_triangle / math.sqrt(3))
r_circle = math.sqrt(s_circle / math.pi)
h_triangle = side_triangle * math.sqrt(3) / 2
r_in = h_triangle / 3
R_out = 2 * h_triangle / 3
print("да" if 2 * r_circle <= side_triangle else "нет")
print("да" if side_triangle <= 2 * r_circle else "нет")

