###4.127
a,b,c = map(int, input().split())
if a > b and a > c:
    print("первое наибольшее")
elif b > a and b > c:
    print("второе наибольшее")
else:
    print("третье наибольшее")
if a < b and a < c:
    print("первое наименьшее")
elif b < a and b < c:
    print("второе наименьшее")
else:
    print("третье наименьшее")
sorted_nums = sorted([a,b,c])
print(sorted_nums[1])

