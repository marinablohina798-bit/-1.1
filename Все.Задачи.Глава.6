import math
###(a)
def f(x):
    return x**4 + 2*x**3 - x - 1
a, b = 0, 1
while b - a > 0.001:
    c = (a + b) / 2
    if f(a) * f(c) < 0:
        b = c
    else:
        a = c
print("Корень (a):", (a+b)/2)
###(б)
def f2(x):
    return x**3 - 0.2*x**2 - 0.2*x - 1.2
a, b = 1, 1.5
while b - a > 0.001:
    c = (a + b) / 2
    if f2(a) * f2(c) < 0:
        b = c
    else:
        a = c
print("Корень (б):", (a+b)/2)

